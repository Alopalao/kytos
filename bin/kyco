#!/usr/bin/env python3

#from kyco.daemon import KycoDaemon
from kyco.config import KycoConfig
from kyco.controller import Controller


def main():
    config = KycoConfig()
#    daemon = KycoDaemon(config.args)
#    daemon.run()

    controller = Controller(config.options['daemon'])
    controller.start()

if __name__ == '__main__':
    main()
